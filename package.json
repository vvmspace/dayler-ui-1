{
  "name": "dayler-ui",
  "version": "2.0.0",
  "main": "./umd/index.min.js",
  "scripts": {
    "dist": "webpack --config ./webpack/index.js",
    "lib": "webpack --config ./webpack/webpack.lib.js",
    "lint": "eslint .",
    "lint:affected": "LIST=`git diff-index --name-only HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then eslint $LIST --no-ignore; fi",
    "start": "node ./server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iknpx/dayler-ui.git"
  },
  "author": "Iakov Salikov <iknpx@aol.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iknpx/dayler-ui/issues"
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint:affected",
      "pre-push": "yarn test"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js",
      "src/app/**/*.js",
      "src/main.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|styl|svg|example)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup/index.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "__tests__/setup"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-feather": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "autoprefixer": "^8.6.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-dayler": "^1.0.3",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.0",
    "gulp-sftp": "^0.1.5",
    "helmet": "^3.12.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11",
    "jest-static-stubs": "^0.0.1",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.2.0",
    "rupture": "^0.7.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  }
}
